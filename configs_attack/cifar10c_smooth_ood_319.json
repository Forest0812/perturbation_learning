{
  "attack": {
      "type": "cvae_predict",
      "sigma": 3.19,
      "model": {
          "type": "cifar10_rectangle", 
          "output_distribution": "gaussian", 
          "latent_dim": 512,
          "nblocks": 4, 
          "big_ch": 64, 
          "small_ch": 16, 
          "kernel_size": 3, 
          "padding": 1
      }, 
      "checkpoint": "experiments/cifar10_c_rectangle_long_cropflip/checkpoints/checkpoint_latest.pth", 
      
      "n_classes": 10, 
      "alpha": 0.001, 
      "selection_n0": 100, 
      "estimation_n": 10000
  },
  "topk": 1,
  "criterion": "cross_entropy", 
  "device": "cuda",


  "training": {
      "batch_size": 128
  },

  "eval": {
      "batch_size": 128
  },

  "dataset": {
      "type": "cifar10c_all_nooriginal",
      "data_path": "../data", 
      "corruptions":[
          "gaussian_blur", 
          "spatter", 
          "saturate"
      ], 
      "severity":[5],
      "k": 3,
      "cropflip": {
          "pad": 4, 
          "crop": [32,32], 
          "flip": true
      }
  }
}